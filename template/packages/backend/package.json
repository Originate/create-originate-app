{
  "name": "@/backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "_tsnode": "ts-node --transpile-only",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:start": "docker start package_name-postgres 2>/dev/null || docker run -e POSTGRES_PASSWORD=password -p 5432:5432 -d --name package_name-postgres postgres:latest",
    "db:stop": "docker stop package_name-postgres",
    "db:destroy": "yarn db:stop >/dev/null && docker rm package_name-postgres",
    "db:shell": "docker exec -it package_name-postgres psql -U postgres",
    "db:migration:generate": "yarn _tsnode scripts/migrationGenerate.ts",
    "typeorm": "yarn _tsnode node_modules/typeorm/cli.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn db:start && nest start --watch",
    "start:debug": "yarn db:start && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --selectProjects unit --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --selectProjects e2e",
    "test:unit": "jest --selectProjects unit",
    "typecheck": "echo backend && tsc --pretty"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.5",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@originate/docker-await-postgres": "^1.1.0",
    "apollo-server-express": "^2.18.2",
    "class-transformer": "^0.3.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.12.2",
    "dataloader": "^2.0.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^7.0.2",
    "lodash": "^4.17.20",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-graphql-plugin": "^2.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
