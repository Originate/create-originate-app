{
  "name": "@/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn start:dev",
    "build": "next build",
    "start": "next start",
    "start:dev": "concurrently -k 'next dev' 'yarn codegen:watch'",
    "codegen": "ts-graphql-plugin typegen",
    "codegen:watch": "nodemon --ext ts,tsx --exec yarn codegen --ignore '/__generated__/'",
    "typecheck": "echo next && tsc -p .",
    "typecheck:watch": "yarn typecheck --watch",
    "lint": "eslint --cache --ext .ts,.tsx --ignore-pattern __generated__ .",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "echo 'TODO: frontend tests'"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@graphql-typed-document-node/core": "^3.1.0",
    "graphql": "^15.4.0",
    "next": "^10.0.0",
    "nodemon": "^2.0.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@hallettj/eslint-plugin-ts-graphql": "^0.1.1",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "^8.2.0",
    "babel-preset-react-app": "^10.0.0",
    "concurrently": "^5.3.0",
    "eslint-plugin-graphql": "^4.0.0",
    "storybook-css-modules-preset": "^1.0.2",
    "ts-graphql-plugin": "^2.0.2",
    "typescript-plugin-css-modules": "^2.8.0"
  }
}
