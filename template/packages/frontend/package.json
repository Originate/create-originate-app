{
  "name": "@/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k 'yarn start' 'yarn codegen:watch'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "yarn workspace @/backend run graphql:schema && ts-graphql-plugin typegen",
    "codegen:watch": "watch 'yarn codegen' src/graphql/",
    "typecheck": "echo react && tsc -p .",
    "typecheck:watch": "yarn typecheck --watch",
    "lint": "eslint --cache --ext .ts,.tsx ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "@types/jest": "^24.0.0",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.8.1",
    "graphql": "^15.3.0",
    "io-ts": "^2.2.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "ts-graphql-plugin": "^2.0.1",
    "watch": "^1.0.2"
  }
}
